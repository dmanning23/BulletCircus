<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
	<metadata>
		<id>BulletCircus</id>
		<version>2.0.7</version>
		<title>BulletCircus</title>
		<authors>dmanning23</authors>
		<owners>dmanning23</owners>
		<projectUrl>https://github.com/dmanning23/BulletCircus</projectUrl>
		<licenseUrl>https://github.com/dmanning23/BulletCircus/blob/master/LICENSE.txt</licenseUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>MonoGame library game that mixes BulletML with flocking and collision detection</description>
		<releaseNotes />
		<copyright>Copyright 2016</copyright>
		<tags>pcl, windows phone, winphone, android, ios, windows universal, monogame</tags>
		<dependencies>

			<!--Xamarin.Android-->
			<group targetFramework="MonoAndroid10">
				<dependency id="MonoGame.Framework.Android" version="3.6"/>
				<dependency id="BulletMLLib" version="1.0"/>
				<dependency id="CellSpacePartition" version="1.0"/>
				<dependency id="CollisionBuddy" version="1.0"/>
				<dependency id="Equationator" version="1.0"/>
				<dependency id="GameTimer" version="1.0"/>
				<dependency id="MatrixExtensions" version="1.0.1"/>
				<dependency id="PrimitiveBuddy" version="1.0"/>
				<dependency id="RandomExtensions.dmanning23" version="1.0"/>
				<dependency id="RectangleF" version="1.0"/>
				<dependency id="Vector2Extensions" version="1.0.1"/>
			</group>

			<!--Xamarin.iOS Unified-->
			<group targetFramework="Xamarin.iOS10">
				<dependency id="MonoGame.Framework.iOS" version="3.6"/>
				<dependency id="BulletMLLib" version="1.0"/>
				<dependency id="CellSpacePartition" version="1.0"/>
				<dependency id="CollisionBuddy" version="1.0"/>
				<dependency id="Equationator" version="1.0"/>
				<dependency id="GameTimer" version="1.0"/>
				<dependency id="MatrixExtensions" version="1.0.1"/>
				<dependency id="PrimitiveBuddy" version="1.0"/>
				<dependency id="RandomExtensions.dmanning23" version="1.0"/>
				<dependency id="RectangleF" version="1.0"/>
				<dependency id="Vector2Extensions" version="1.0.1"/>
			</group>

			<group targetFramework="net451">
				<dependency id="MonoGame.Framework.WindowsDX" version="3.6"/>
				<dependency id="BulletMLLib" version="1.0"/>
				<dependency id="CellSpacePartition" version="1.0"/>
				<dependency id="CollisionBuddy" version="1.0"/>
				<dependency id="Equationator" version="1.0"/>
				<dependency id="GameTimer" version="1.0"/>
				<dependency id="MatrixExtensions" version="1.0.1"/>
				<dependency id="PrimitiveBuddy" version="1.0"/>
				<dependency id="RandomExtensions.dmanning23" version="1.0"/>
				<dependency id="RectangleF" version="1.0"/>
				<dependency id="Vector2Extensions" version="1.0.1"/>
			</group>

		</dependencies>
	</metadata>
	<files>

		<!--Xamarin.Android-->
		<file src="BulletCircus.Android\bin\Release\BulletCircus.Android.pdb" target="lib\MonoAndroid10\BulletCircus.Android.pdb" />
		<file src="BulletCircus.Android\bin\Release\BulletCircus.Android.dll" target="lib\MonoAndroid10\BulletCircus.Android.dll" />

		<!--Xamarin.iOS Unified-->
		<file src="BulletCircus.iOS\bin\Release\BulletCircus.iOS.pdb" target="lib\Xamarin.iOS10\BulletCircus.iOS.pdb" />
		<file src="BulletCircus.iOS\bin\Release\BulletCircus.iOS.dll" target="lib\Xamarin.iOS10\BulletCircus.iOS.dll" />

		<!--WindowsDX-->
		<file src="BulletCircus.WindowsDX\bin\Release\BulletCircus.WindowsDX.pdb" target="lib\net451\BulletCircus.WindowsDX.pdb" />
		<file src="BulletCircus.WindowsDX\bin\Release\BulletCircus.WindowsDX.dll" target="lib\net451\BulletCircus.WindowsDX.dll" />

	</files>
</package>
